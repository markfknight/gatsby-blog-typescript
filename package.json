{
  "name": "gatsby-blog-typescript",
  "private": true,
  "description": "Starter site for a simple Gatsby blog with TypeScript",
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx,.js src",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll"
  },
  "dependencies": {
    "gatsby": "^2.24.89",
    "gatsby-plugin-manifest": "^2.4.36",
    "gatsby-plugin-offline": "^3.2.37",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.19.2",
    "@types/react": "^16.9.54",
    "@types/react-dom": "^16.9.9",
    "@types/react-helmet": "^6.1.0",
    "@types/react-test-renderer": "^16.9.3",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "babel-jest": "^26.6.1",
    "babel-preset-gatsby": "^0.5.14",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.1",
    "prettier": "2.1.2",
    "react-test-renderer": "^17.0.1",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://https://github.com/markfknight/gatsby-blog-typescript"
  },
  "bugs": {
    "url": "https://https://github.com/markfknight/gatsby-blog-typescript/issues"
  }
}
